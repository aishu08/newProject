<div class="progress-container animate-show-hide" flex layout="row" layout-align="center center" ng-show="makingAjaxCall">
    <md-progress-circular md-diameter="75"></md-progress-circular>
</div>
<md-dialog aria-label="Autocomplete Dialog">
    <md-toolbar>
        <div class="md-toolbar-tools">
            Generate VPN file
        </div>
    </md-toolbar>

    <md-dialog-content class="md-dialog-content  " ng-cloak>
        <form name="dialogform" ng-submit="submit()">
            <md-input-container class="md-block input_container m-t-b">
                <!-- <label>product</label> -->
                <md-select placeholder="Product" ng-model="chosenOption">
                    <md-option ng-repeat="option in options" ng-value="option">
                        {{option}}
                    </md-option>
                </md-select>
            </md-input-container>
    </md-dialog-content>

    <!-- <span>Customer Name</span> -->
    <div flex>
        <md-input-container class="md-block input_container">
            <input required placeholder="Customer Name" type="text" ng-model="name" />
        </md-input-container>
    </div>
    <!-- <span>Location</span> -->
    <div flex>
        <md-input-container class="md-block input_container">
            <input required placeholder="Location" type="text" ng-model="location" />
        </md-input-container>
    </div>

    <!-- <span>Device Number</span> -->
    <div flex>
        <md-input-container class="md-block input_container">
            <input required placeholder="Device Number" type="number" ng-model="device_number" />
        </md-input-container>
    </div>


    <md-dialog-actions>
        <md-button class="md-primary md-raised " type="submit" value="Submit">Generate File</md-button>
        <!-- <md-button class="md-primary md-raised " ng-click="dowloadFile() " ng-disabled="true">Download</md-button> -->
        <!-- <md-button class="md-primary md-raised " ng-click="emailSend() " ng-disabled="true">Email</md-button> -->
        <md-button class="md-primary md-raised " ng-click="delete() ">Cancel</md-button>
    </md-dialog-actions>
    </form>
</md-dialog>