<md-dialog aria-label="Autocomplete Dialog Example" flex=50>
    <div class="md-toolbar-tools border-bottom">
        <span style="color: #000;">Available Certificates</span>
    </div>
    <md-dialog-content class="md-dialog-content text-dark" ng-cloak>
        <h2>Select Certificate</h2>
        <div layout="row" flex>

            <md-input-container class=" md-block" style="padding-right:2%;padding-left:2%;min-width:2%">

                <h2>#</h2>

            </md-input-container>

            <md-input-container class="md-block" layout="row" layout-align="center center" style="width:100%">
                <div flex layout="row" layout-align="stretch" style="width:100%">
                    <div flex layout="column" layout-align="stretch" style="width:100%">
                        <h2>Certificates</h2>

                    </div>
                    <div flex layout="column" layout-align="stretch">
                        <h2>Public Keys</h2>

                    </div>
                    <div flex layout="column" layout-align="stretch">
                        <h2>Private Keys</h2>

                    </div>
                </div>

            </md-input-container>
        </div>
        <hr>
        <div layout="row" flex ng-repeat="c in certificateslist" ng-if="!c.selected">

            <div class=" md-block md-padding" style="vertical-align:center">

                <input type="radio" ng-model="certs" ng-click="certselection(c)" ng-value="c" style="height:100%;width:30px">

            </div>

            <md-input-container class="md-block" layout="row" layout-align="center center" style="width:100%">
                <div flex layout="row" layout-align="stretch" style="width:100%">
                    <div flex layout="column" layout-align="stretch" style="width:100%">

                        <h4>{{c.crt}}</h4>
                    </div>
                    <div flex layout="column" layout-align="stretch">

                        <h4>{{c.pem}}</h4>
                    </div>
                    <div flex layout="column" layout-align="stretch">

                        <h4>{{c.ppk}}</h4>
                    </div>
                </div>

            </md-input-container>
        </div>
    </md-dialog-content>
    <md-dialog-actions>
        <md-button class="md-primary md-raised" ng-click="setcertificate()">Ok</md-button>
        <md-button class="md-primary md-raised" ng-click="closeDialog()">Cancel</md-button>
    </md-dialog-actions>
</md-dialog>