<md-dialog aria-label="Seat Config" layout-padding>
    <h4>Seat Configuration</h4>


    <md-content layout="row" layout-align="space-around center" ng-form="seatform">
        <md-input-container>
            <input required md-no-asterisk type="number" name="size" ng-model="seatsetting.size" />
        </md-input-container>
        <md-input-container>
            <md-select placeholder="Select seat type" class="md-padding" ng-model="seatsetting.type">
                <md-optgroup label="Seat Type">
                    <md-option value="Circular">Circular</md-option>
                    <md-option value="Square">Square</md-option>
                </md-optgroup>
            </md-select>
        </md-input-container>
        <md-input-container>
            <div flex layout="column" layout-align="center stretch">
                <md-button class="md-raised md-primary" ng-click="Saveseatsetting(seatsetting,seatform)">Save</md-button>
            </div>
        </md-input-container>

        <md-input-container flex>
            <md-button class="md-raised md-primary" ng-click="done()">
                Cancel
            </md-button>
        </md-input-container>
    </md-content>
</md-dialog>