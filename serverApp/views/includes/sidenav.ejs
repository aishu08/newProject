    <a class="alink">
        <div layout="column" layout-align="center center" class="activecircle" ng-click="changeView('seatDashboard')">
            <div ng-class="{ 'stick-horizontal' : pageActive == 'seatDashboard' }"></div>
            <div class="round service">
                <div class="service-circle" ng-class="{ 'active' : pageActive == 'seatDashboard' }">
                    <div class="circle">
                        <div class="innercircle"></div>
                    </div>
                    <i class="material-icons menu2 text-white">event_seat</i>
                    <md-tooltip direction="bottom" class="md-tooltip md-tooltip-bottom">Seat Allocation</md-tooltip>
                </div>
            </div>
        </div>
    </a>
