<!-- <md-sidenav class="md-sidenav-left dark-side-nav" flex="15" style="height: 100%" md-component-id="sidenav-left" md-is-locked-open="$mdMedia('gt-md')"> -->
<md-sidenav flex="15" md-component-id="left" class="md-sidenav-left md-whiteframe-glow-z1 dark-side-nav" style="height: 100%"
  md-is-locked-open="$mdMedia('gt-sm')">
  <div class="ad-dark-content">
    <div>
      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <h3 class="highlight-heading">{{selectedbulding.name}}</h3>

      </md-input-container>

    </div>
    <hr>
    <div class="padding">
      <md-input-container class="no-margin" layout="row" layout-align="center center">

        <h3 class="highlight-heading">Configure Seats</h3>
      </md-input-container>
      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="addseat()">Add Seat</md-button>
        </div>
      </md-input-container>

      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="choosetemplate()">Upload Seats</md-button>
        </div>
      </md-input-container>

      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="addroom()">Add Room</md-button>
        </div>
      </md-input-container>
      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="addbu()">Add Bu</md-button>
        </div>
      </md-input-container>

      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="SendToHost()">Send To Host</md-button>
        </div>
      </md-input-container>

      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ng-click="addseatsettiongs()">Settings</md-button>
        </div>
      </md-input-container>

      <md-input-container class="no-margin" layout="row" layout-align="center center">
        <div flex layout="column" layout-align="center stretch">
          <md-button class="md-raised md-primary light-text" ui-sref="commission.floor">Back</md-button>
        </div>
      </md-input-container>
    </div>
  </div>
</md-sidenav>