<div layout="row" flex="50">

    <md-dialog aria-label="Autocomplete Dialog Example" class="md-padding" flex=100 style="background-color: rgb(43, 43, 43)">
        <div class="progress-container animate-show-hide" flex layout="row" layout-align="center center" ng-show="buttonclicked">
            <md-progress-circular md-diameter=""></md-progress-circular>
        </div>

        <form name="lightform">

            <div layout="row" class="md-toolbar-tools">
                <div flex="20">
                    <img src="img/light-bulb.png" class="dialogimage"></div>
                <h3 flex="80" class="border-bottom dialogheader">Add Devices</h3>
            </div>
            <md-dialog-content>
                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak style="overflow-y:scroll;max-height:450px">
                    <h5 class="border-bottom">Add Lights</h5>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak ng-if="lighttypes.length>0" style="overflow-y:scroll;max-height:450px">
                    <div layout="row" layout-wrap layout-align="start center" ng-repeat="lighttype in lighttypes">
                        <div flex=40 flex-sm=100 flex-xs=100 flex-md=40 class="text-white">
                            <div layout="row" layout-wrap layout-align="start center">
                                <div flex=30 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <!-- ----{{lighttype}} -->
                                    <div class="light {{lighttype.shape}}" style="position: inherit;" ng-if="lighttype.shape === selected.class && lighttype._id===selected.id" ng-style="lighttype.styobj"></div>
                                    <div class="light {{lighttype.shape}}" style="position: inherit;" ng-if="lighttype.shape === selected.class && lighttype._id!==selected.id"></div>
                                    <div class="light {{lighttype.shape}}" style="position: inherit;" ng-if="lighttype.shape !== selected.class"></div>
                                </div>
                                <div flex=20 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <md-checkbox aria-label="Select Shapes" ng-model="lighttype.selected" style="margin-bottom:0px">

                                    </md-checkbox>
                                </div>
                                <div flex=50 flex-sm=60 flex-xs=60 flex-md=60 class="text-white">
                                    <md-input-container class="text-white" style="margin:0;width:90%;overflow-wrap: break-word;">
                                        {{lighttype.name}}
                                    </md-input-container>
                                </div>

                            </div>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input ng-required="lighttype.selected" type="number" ng-model="lighttype.nos" placeholder="No. of Lights" ng-disabled="!lighttype.selected" style="border-bottom: solid;border-bottom-width: 0.1px;" min="0">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input type="number" name="width_{{$index}}" ng-model="lighttype.width" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="demolightshape(lighttype,lightform)" max="45" placeholder="Light Width" limit-to-max-light ng-disabled="!lighttype.selected"
                                    min="10">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0">
                                <input type="number" name="height_{{$index}}" ng-model="lighttype.height" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="demolightshape(lighttype,lightform)" max="45" placeholder="Light Height" limit-to-max-light ng-disabled="!lighttype.selected"
                                    min="10">

                            </md-input-container>
                        </div>
                    </div>
                </md-dialog-content>





                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak style="overflow-y:scroll;max-height:450px">
                    <h5 class="border-bottom">Add Tunable Light</h5>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak ng-if="lighttypes.length>0" style="overflow-y:scroll;max-height:450px">
                    <div layout="row" layout-wrap layout-align="start center" ng-repeat="tunable in lighttypes">
                        <div flex=40 flex-sm=100 flex-xs=100 flex-md=40 class="text-white">
                            <div layout="row" layout-wrap layout-align="start center">
                                <div flex=30 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <!-- ----{{lighttype}} -->
                                    <div class="light {{tunable.shape}}" style="position: inherit;" ng-if="tunable.shape === selected.class && tunable._id===selected.id" ng-style="tunable.tunstyobj"></div>
                                    <div class="light {{tunable.shape}}" style="position: inherit;" ng-if="tunable.shape === selected.class && tunable._id!==selected.id"></div>
                                    <div class="light {{tunable.shape}}" style="position: inherit;" ng-if="tunable.shape !== selected.class"></div>
                                </div>
                                <div flex=20 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <md-checkbox aria-label="Select Shapes" ng-model="tunable.tunSelected" style="margin-bottom:0px">

                                    </md-checkbox>
                                </div>
                                <div flex=50 flex-sm=60 flex-xs=60 flex-md=60 class="text-white">
                                    <md-input-container class="text-white" style="margin:0;width:90%;overflow-wrap: break-word;">
                                        {{tunable.name}}
                                    </md-input-container>
                                </div>

                            </div>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input ng-required="tunable.tunSelected" type="number" ng-model="tunable.tunNos" placeholder="No. of Lights" ng-disabled="!tunable.tunSelected" style="border-bottom: solid;border-bottom-width: 0.1px;" min="0">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input type="number" name="width_{{$index}}" ng-model="tunable.tunWidth" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="demoTubLightShape(tunable,lightform)" max="45" placeholder="Light Width" limit-to-max-light ng-disabled="!tunable.tunSelected"
                                    min="10">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0">
                                <input type="number" name="height_{{$index}}" ng-model="tunable.tunHeight" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="demoTubLightShape(tunable,lightform)" max="45" placeholder="Light Height" limit-to-max-light ng-disabled="!tunable.tunSelected"
                                    min="10">

                            </md-input-container>
                        </div>
                    </div>
                </md-dialog-content>






                <md-dialog-content class="md-dialog-content text-white" ng-cloak layout-align="center center" ng-if="lighttypes.length<=0">
                    <h3>Please Configure Light Types</h3>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak style="overflow-y:scroll;max-height:450px">
                    <h5 class="border-bottom">Add Sensors</h5>
                </md-dialog-content>

                <md-dialog-content class="md-dialog-content text-white" ng-cloak ng-if="sensortypes.length>0" style="overflow-y:scroll;max-height:450px">
                    <div layout="row" layout-wrap layout-align="start center" ng-repeat="sensortype in sensortypes">
                        <div flex=40 flex-sm=100 flex-xs=100 flex-md=40 class="text-white">
                            <div layout="row" layout-wrap layout-align="start center">
                                <div flex=30 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <div class="sensor {{sensortype.shape}}" style="position: inherit;" ng-if="sensortype.shape === selected.class && sensortype._id===selected.id" ng-style="sensortype.styobj"></div>
                                    <div class="sensor {{sensortype.shape}}" style="position: inherit;" ng-if="sensortype.shape === selected.class && sensortype._id!==selected.id"></div>
                                    <div class="sensor {{sensortype.shape}}" style="position: inherit;" ng-if="sensortype.shape !== selected.class"></div>
                                </div>
                                <div flex=20 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <md-checkbox aria-label="Select Shapes" ng-model="sensortype.selected" style="margin-bottom:0px">

                                    </md-checkbox>
                                </div>
                                <div flex=50 flex-sm=60 flex-xs=60 flex-md=60 class="text-white">
                                    <md-input-container class="text-white" style="margin:0;overflow-wrap: break-word;">
                                        {{sensortype.name}}
                                    </md-input-container>
                                </div>
                            </div>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input ng-required="sensortype.selected" type="number" ng-model="sensortype.nos" placeholder="No. of Sensors" ng-disabled="!sensortype.selected" style="border-bottom: solid;border-bottom-width: 0.1px;" min="0">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input type="number" name="width_{{$index}}" ng-model="sensortype.width" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="changeSensorShape(sensortype,lightform)" max="45" placeholder="Dimension" limit-to-max-light ng-disabled="!sensortype.selected"
                                    min="10">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input type="number" ng-model="sensortype.timeout" style="border-bottom: solid;border-bottom-width: 0.1px;" max="60" placeholder="Time Out(in mins)" limit-to-max-light ng-disabled="!sensortype.selected" min="5">
                            </md-input-container>
                        </div>
                    </div>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content text-white" ng-cloak layout-align="center center" ng-if="sensortypes.length<=0">
                    <h3>Please Configure Sensor Types</h3>
                </md-dialog-content>

                <md-dialog-content class="md-dialog-content-p10 text-white" ng-cloak style="overflow-y:scroll;max-height:450px">
                    <h5 class="border-bottom">Add Touch Panels</h5>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content text-white" ng-cloak ng-if="tptypes.length>0" style="overflow-y:scroll;max-height:450px">
                    <div layout="row" layout-wrap layout-align="start center" ng-repeat="tptype in tptypes">
                        <div flex=40 flex-sm=100 flex-xs=100 flex-md=40 class="text-white">
                            <div layout="row" layout-wrap layout-align="start center">
                                <div flex=30 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <div class="sensor {{tptype.shape}}" style="position: inherit;" ng-if="tptype.shape === selected.class && tptype._id===selected.id" ng-style="tptype.styobj"></div>
                                    <div class="sensor {{tptype.shape}}" style="position: inherit;" ng-if="tptype.shape === selected.class && tptype._id!==selected.id"></div>
                                    <div class="sensor {{tptype.shape}}" style="position: inherit;" ng-if="tptype.shape !== selected.class"></div>
                                </div>
                                <div flex=20 flex-sm=20 flex-xs=20 flex-md=20 class="text-white">
                                    <md-checkbox aria-label="Select Shapes" ng-model="tptype.selected" style="margin-bottom:0px">

                                    </md-checkbox>
                                </div>
                                <div flex=50 flex-sm=60 flex-xs=60 flex-md=60 class="text-white">
                                    <md-input-container class="text-white" style="margin:0;overflow-wrap: break-word;">
                                        {{tptype.name}}
                                    </md-input-container>
                                </div>
                            </div>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input ng-required="tptype.selected" type="number" ng-model="tptype.nos" placeholder="No. of Touch Panels" ng-disabled="!tptype.selected" style="border-bottom: solid;border-bottom-width: 0.1px;" min="0">
                            </md-input-container>
                        </div>
                        <div flex=20 flex-sm=50 flex-xs=50 flex-md=50>
                            <md-input-container class="text-white" style="margin:0;width:90%;">
                                <input type="number" name="width_{{$index}}" ng-model="tptype.width" style="border-bottom: solid;border-bottom-width: 0.1px;" ng-change="changeSensorShape(tptype,lightform)" max="45" placeholder="Dimension" limit-to-max-light ng-disabled="!tptype.selected"
                                    min="10">
                            </md-input-container>
                        </div>
                    </div>
                </md-dialog-content>
                <md-dialog-content class="md-dialog-content text-white" ng-cloak layout-align="center center" ng-if="tptypes.length<=0">
                    <h3>Please Configure Touch Panel Types</h3>
                </md-dialog-content>



                <md-dialog-actions layout="row">
                    <md-button class="md-raised md-primary" ng-click="closeDialog()">Cancel</md-button>
                    <md-button class="md-raised md-primary" ng-disabled="buttonclicked || lighttypes.length<=0" ng-click="createdevices(lighttypes,sensortypes,tptypes,lightform)">Done</md-button>
                </md-dialog-actions>
            </md-dialog-content>
        </form>

    </md-dialog>
</div>